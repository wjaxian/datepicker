  "version": "1.0.5",
  "author": "wujia <web@wujiabk.com>",
  "main": "dist/Wdatepicker.umd.min.js",
  "style": "dist/Wdatepicker.css",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wujiabk/datepicker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wujiabk/datepicker"
  },
  "keywords": [
    "vue-datepicler",
    "datepicker",
    "w-datepicker",
    "Wdatepicker",
    "@wujiaxian/datepicker"
  ],
  "private": false,
  "scripts": {
